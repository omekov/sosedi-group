{
    "swagger": "2.0",
    "info": {
        "description": "REST API for Sosedi Group API",
        "title": "Sosedi Group API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/rest/counter/add/{num}": {
            "post": {
                "description": "Redis increment",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "counter"
                ],
                "summary": "Counter Add (increment)",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Counter number",
                        "name": "num",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/rest/counter/sub/{num}": {
            "post": {
                "description": "Redis decrement",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "counter"
                ],
                "summary": "Counter Sub (decrement)",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Counter number",
                        "name": "num",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/rest/counter/val": {
            "get": {
                "description": "Redis get num",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "counter"
                ],
                "summary": "Counter Val",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
